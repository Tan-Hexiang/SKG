{
	"training_data": "../../datasets/",
  	"output": "../../output/results/",
  	"dataset_division": "721_5fold",
	"train_ratio": 0.7,
	"valid_ratio":0.1,
	"align_ratio": 1,

  	"embedding_module": "RDGCN",
  	"alignment_module": "mapping",
	"search_module": "greedy",

    "dim": 100,

	"neg_sampling": "uniform",
	"neg_triple_num": 2,

	"learning_rate": 0.01,
	"max_epoch": 100,
	"batch_size": 64,

	"batch_threads_num": 2,
	"test_threads_num": 3,

	"ordered": true,
	"start_valid": 30,
	"eval_freq": 10,
	"stop_metric": "mrr",
	"eval_metric": "manhattan",
	"csls": 10,
	"top_k": [1, 5, 10, 20, 50],
	"is_save": true,
	"eval_norm": false,

    "gamma": 1.0,
    "dropout": 0,
	"beta": 0.1,
    "alpha": 0.1
}